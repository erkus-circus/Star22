func main: int() {
    var x: int = 0;

    while 1 {
        var p: str = input();
        print(intToString(pow(2,x)));
        x = x + 1;
    }
    return 0;
}

func pow: int(base: int, power: int) {
    var res: int = 1;
    var index: int = 0;
    if power == 0 {
        return 1;
    }
    if power == 1 {
        return base;
    }

    while index < power {
        res = res * base;
        index = index + 1;

    }
    return res;
}