function max(numOne: int, numTwo: int): int {
    if numOne > numTwo {
        return numOne;
    }
    return numTwo;
}

function strComp(strOne: string, strTwo: string): bool {
    if size(strOne) != size(strTwo) {
        return 0;
    }
    var i: int = 0;
    while i < size(strOne) {
        if getIndex(strOne, i) != getIndex(strTwo, i) {
            return 0;
        }
    }
    return 1;

}